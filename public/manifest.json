{
  "manifest_version": 3,
  "name": "img-table_extension",
  "version": "1.0",
  "description": "Download images and tables from the web",
  "action":{
    "default_popup": "index.html",
    "default_title": "Download"
  },
  "permissions":["downloads","activeTab","scripting"],
  "host_permissions":[
    "<all_urls>"
  ],
  "background":{
    "service_worker": "background.js"
  }
  
}